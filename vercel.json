{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "chatapp/next.config.ts",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "chatapp/.next/server/pages/$1"
    }
  ]
}
